<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Site_Code_Numeric_only</fullName>
    <active>true</active>
    <description>Site Code must be numeric only</description>
    <errorConditionFormula>NOT(REGEX(  Site_Code__c  ,&quot;[0-9]{0,10}&quot;))</errorConditionFormula>
    <errorMessage>Site Code must be Numeric.</errorMessage>
</ValidationRule>
