<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>service_type_field_check_generic</fullName>
    <active>true</active>
    <errorConditionFormula>IF((ISPICKVAL(Family,&apos;Generic&apos;)), 
ISPICKVAL(Service_Type__c, &apos;Acute (Daily)&apos;) = False , 
ISPICKVAL(Service_Type__c, &apos;Acute (Daily)&apos;) = True )</errorConditionFormula>
    <errorDisplayField>Service_Type__c</errorDisplayField>
    <errorMessage>Please make sure to only select &quot;Acute (Daily)&quot; under Service Type if Product Family is &quot;Generic&quot; otherwise Acute (Daily) cant be selected.</errorMessage>
</ValidationRule>
