<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DoD_should_be_Greater_Than_DoA</fullName>
    <active>true</active>
    <errorConditionFormula>$Setup.Data_Migration_Check__c.Bypass_Flow_Validations__c = FALSE &amp;&amp;
  Date_of_Discharge__c &lt;  Date_of_Admission__c</errorConditionFormula>
