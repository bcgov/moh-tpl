<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Of_Discharge_Cannot_Be_In_Future</fullName>
    <active>true</active>
    <description>Date of discharge must be greater than date of admission</description>
    <errorConditionFormula>$Setup.Data_Migration_Check__c.Bypass_Flow_Validations__c = FALSE &amp;&amp; ISBLANK(Source_System_ID__c)  &amp;&amp;
   Date_of_Discharge__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date_of_Discharge__c</errorDisplayField>
    <errorMessage>Date Of Discharge Cannot Be In Future</errorMessage>
</ValidationRule>
